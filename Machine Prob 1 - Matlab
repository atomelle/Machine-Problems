%Machine Problem 1

function a = machineprob1()
f =(0:99);
for n=1:100
    if f(n)<9;
        f(n)=(f(n)^2)-7;
    elseif f(n)== 9;
        f(n)= NaN;
    elseif f(n)>= 10;
        f(n)= f(n-10);
        
    end
    stem(f);                   % stem( is used to show curves in Matlab 
end

% Labels of the graphs

grid()
title('Problem 1: Stem()')
xlabel('x-axis')                        
ylabel('y-axis')                        



        
